{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.cjs\"",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "api:migrate": "sequelize db:migrate",
    "api:migrate:undo": "sequelize db:migrate:undo:all",
    "api:migrate:generate": "sequelize migration:generate --name auto",
    "api:seed": "sequelize db:seed:all",
    "api:seed:undo": "sequelize db:seed:undo:all",
    "api:seed:generate": "sequelize seed:generate --name auto"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@react-email/render": "^0.0.13",
    "@sendgrid/mail": "^8.1.3",
    "@tma.js/init-data-node": "^1.2.7",
    "@types/base-64": "^1.0.2",
    "@types/multer": "^1.4.11",
    "@types/node-telegram-bot-api": "^0.64.6",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "formidable": "^3.5.1",
    "fs": "0.0.1-security",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.10.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "passport": "^0.7.0",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "short-unique-id": "^5.2.0",
    "socket.io": "^4.7.5",
    "telegraf": "^4.16.3", 
    "telegram": "^2.22.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@repo/email-template": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/i18n": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/util": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
